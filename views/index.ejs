<% include partials/header %>

<div class="container">
  <h1>Recent Posts</h1>
  <% blogs.forEach(function(blog) { %>
    <div class="blog-post">
      <h2><%= blog.title %></h2>
      <img src="<%= blog.image %>" alt="">
      <span><%= blog.created %></span>
      <p><%= blog.body %></p>
    </div>
  <% }); %>
</div>

<% include partials/footer %>